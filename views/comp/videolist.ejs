<% if (!video) return; %>
<% if (video.type.endsWith("Video")) { %><% if (video.author) { %><% if (video.author?.thumbnails.length) { %><div class="video-query"><div class="video-related"><a href="/w/<%= video.id %>">
<div class="media-item-thumbnail"><div class="content-miniature"><div class="cover"></div><img style="background-color:#FBFAFA" src="/vi/<%= video.id %>/mqdefault.jpg" alt="" loading="lazy"/><div class="video-thumbnails-overlay"><small style="margin: 5px;padding: 0px 4px;border-radius: 2px;background-color: black;color:white;font-size:11px;"><%= video.duration.text %></small></div></div></div></a></div></div></div><div class="title-video"></div>
<div class="channel-author"><div class="letras"><div class="avatar-channel"><div class="avatar"><img src="/yt3<%- (new URL(video.author.thumbnails[0].url)).pathname %>" loading="lazy"/></div></div><div class="name-author4"><span class="name"><%= video.title.text %><div class="title-video"><div class="channel-visits-date"><%= video.author.name %> • <%= video.view_count?.text || video.short_view_count?.text %> • <%= video.published.text %><% } %><% } %><% video.badges?.forEach(badge => { %><% }); %></div></div></div></div></div><br><% } else if (video.type.endsWith("Playlist")) { %><div class="video-query"><div class="video-related"><a href="/p/<%= video.id %>"><div class="media-item-thumbnail"><div class="content-miniature"><div class="cover"></div><img loading="lazy" src="/vi/<%= video?.endpoint?.payload?.videoId || video.first_videos[0]?.id %>/mqdefault.jpg"/><div class="video-thumbnails-overlay"><small style="margin: 0px;padding: 100px 80px;border-radius: 2px;background: rgba(34,34,34,0.75);color:white;font-size:11px;"><%= video.video_count?.text %></small></div></div></div></a></div></div></div><div class="title-video"></div><div class="channel-author"><div class="letras"><div class="avatar-channel"><div class="avatar"><img lazy src=""</div></div></div><div class="name-author4"><span class="name"><%= video.title.text %><div class="title-video"><div class="channel-visits-date"><%= video.author.name %> • <%= video.video_count?.text %><div class="title-descrip"></div></div></div></div><% if (video.author) { %><% } %><% video.badges?.forEach(badge => { %><% }); %></div></div><% } else if (video.type.endsWith("Channel")) { %><hr style="height:5px;border-width:0;color:gray;background-color:#DCDCDC">
<div class="channel-author"><div class="var_1"><div class="avattar-cchannel"><div class="avattar">
    <img src="/yt3<%- (new URL(video.author.thumbnails[0].url.startsWith("http") ? video.author.thumbnails[0].url : "https:" + video.author.thumbnails[0].url)).pathname %>" loading="lazy"/></div></div><div class="name-author"><small class="name"><%= video.author.name %></small><div class="channel-visits-date">@<%= video.author.name %><div class="b"><%= video.description_snippet?.text %></div></div><br><div class="var_2"><div class="video-thumbnails-overlay"><small style="margin: 10px;padding: 10px 25px;border-radius: 15px;background-color: black;color:white;font-size:10px;">Suscribirse</small></div></div></div></div><br>

<% } else if (video.type === "Shelf") { %>
  <hr style="height:5px;border-width:0;color:gray;background-color:#DCDCDC"><h4><summary class="shelfTitle"><font color="red">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= video.title.text %></font></summary></h4>
    <% video.content?.items?.forEach(video => { %>
      <%- include("videolist.ejs", { video }); %>
    <% }); %>
  </details>
<% } %>